{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "sampleADF00"
		},
		"LS_ABLB_Excel_Customer_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'LS_ABLB_Excel_Customer'"
		},
		"LS_ABLB_Sales_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'LS_ABLB_Sales'"
		},
		"LS_ABLB_Sales_Meta_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'LS_ABLB_Sales_Meta'"
		},
		"LS_ASQL_Users_password": {
			"type": "secureString",
			"metadata": "Secure string for 'password' of 'LS_ASQL_Users'"
		},
		"ALS_API_Users_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://jsonplaceholder.typicode.com/users"
		},
		"LS_ASQL_Users_properties_typeProperties_server": {
			"type": "string",
			"defaultValue": "sampleserver00.database.windows.net"
		},
		"LS_ASQL_Users_properties_typeProperties_database": {
			"type": "string",
			"defaultValue": "sampleDB00"
		},
		"LS_ASQL_Users_properties_typeProperties_userName": {
			"type": "string",
			"defaultValue": "varsha"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "sampleADF00_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2024-03-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"LS_ABLB_Excel_Customer_connectionString": {
						"value": "[parameters('LS_ABLB_Excel_Customer_connectionString')]"
					},
					"LS_ABLB_Sales_connectionString": {
						"value": "[parameters('LS_ABLB_Sales_connectionString')]"
					},
					"LS_ABLB_Sales_Meta_connectionString": {
						"value": "[parameters('LS_ABLB_Sales_Meta_connectionString')]"
					},
					"LS_ASQL_Users_password": {
						"value": "[parameters('LS_ASQL_Users_password')]"
					},
					"ALS_API_Users_properties_typeProperties_url": {
						"value": "[parameters('ALS_API_Users_properties_typeProperties_url')]"
					},
					"LS_ASQL_Users_properties_typeProperties_server": {
						"value": "[parameters('LS_ASQL_Users_properties_typeProperties_server')]"
					},
					"LS_ASQL_Users_properties_typeProperties_database": {
						"value": "[parameters('LS_ASQL_Users_properties_typeProperties_database')]"
					},
					"LS_ASQL_Users_properties_typeProperties_userName": {
						"value": "[parameters('LS_ASQL_Users_properties_typeProperties_userName')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}